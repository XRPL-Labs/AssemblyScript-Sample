{
  "name": "wasm-node-sample",
  "version": "0.0.1",
  "description": "",
  "main": "run-wasm/as-loader.js",
  "scripts": {
    "test": "echo \"Error: no test specified\" && exit 1",
    "asbuild:untouched": "asc src/assembly/index.ts --debug -b build/as-untouched.wasm -t build/as-untouched.wat -d build/as-untouched.d.ts",
    "asbuild:optimized": "asc src/assembly/index.ts -O --noAssert -b build/as-optimized.wasm -t build/as-optimized.wat -d build/as-optimized.d.ts",
    "asbuild:custom": "asc src/assembly/index.ts -O3 --runtime stub --noAssert -b build/as-optimized.wasm -t build/as-optimized.wat -d build/as-optimized.d.ts",
    "asbuild": "npm run asbuild:untouched && npm run asbuild:optimized",
    "full": "./build-and-run.sh"
  },
  "author": "Wietse Wind <hi@wietse.com>",
  "license": "MIT",
  "dependencies": {
    "@assemblyscript/loader": "^0.14.12"
  },
  "devDependencies": {
    "assemblyscript": "^0.14.12"
  }
}
